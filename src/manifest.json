{
    "name": "Chrome-vue-boilerplate",
    "version": "0.1.0",
    "description": "Build an awesome Chrome extension",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "action": {
        "default_popup": "views/popup.html"
    }
}